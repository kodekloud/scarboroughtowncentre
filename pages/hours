<script>
    $(document).ready(function(e) {
        init(e);
        function renderAll (){
            var hours = getPropertyHours();
            var hour_mf = [];
            $.each(hours, function(key, val){
                if (val.day_of_week == 0){
                    hour_mf.push(val);
                }
            });
            renderHours('#hours_mf_container','#hours_mf_template', hour_mf, 'reg_hours')
            
            show_content();
        }
        loadMallDataCached(renderAll); 
    })
</script>